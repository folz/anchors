{
  "name": "@folz/anchors-monorepo",
  "private": true,
  "version": "0.1.0",
  "description": "Monorepo for @folz/anchors",
  "scripts": {
    "typecheck": "tsc",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "build": "pnpm -r build && pnpm build:typescript",
    "build:typescript": "tsc --project tsconfig.build.json",
    "clean": "rm -rf packages/**/src/[^_]*.d.ts packages/**/src/**/[^_]*.d.ts packages/**/dist",
    "website": "pnpm --filter '@folz/anchors-website'"
  },
  "author": "folz",
  "license": "MIT",
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": false,
    "proseWrap": "always"
  },
  "browserslist": [
    "Chrome >= 73",
    "Firefox >= 78",
    "Edge >= 79",
    "Safari >= 12.0",
    "iOS >= 12.0",
    "Opera >= 53"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/node": "^20.4.3",
    "@types/resize-observer-browser": "^0.1.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.27",
    "prettier": "^2.8.8",
    "rollup": "^3.26.3",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.0.4",
    "vite": "^4.4.12",
    "vitest": "^0.32.4"
  },
  "packageManager": "pnpm@8.6.3"
}
